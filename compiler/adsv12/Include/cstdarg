#ifndef __cstdarg
#define __cstdarg

  #define __STDARG_NO_EXPORTS 1
  #include <stdarg.h>
  #undef __STDARG_NO_EXPORTS

  #ifdef __EDG_RUNTIME_USES_NAMESPACES
    #undef __stdarg_h  // later inclusion of stdarg.h must do using std::*
  #endif

#endif
