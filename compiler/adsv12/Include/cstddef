#ifndef __cstddef
#define __cstddef

  #define __STDDEF_NO_EXPORTS 1
  #include <stddef.h>
  #undef __STDDEF_NO_EXPORTS

  #ifdef __EDG_RUNTIME_USES_NAMESPACES
    #undef __stddef_h  // later inclusion of stddef.h must do using std::*
  #endif

#endif

