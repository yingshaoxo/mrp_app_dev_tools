#ifndef __cerrno
#define __cerrno

  #define __ERRNO_NO_EXPORTS 1
  #include <errno.h>
  #undef __ERRNO_NO_EXPORTS

  #ifdef __EDG_RUNTIME_USES_NAMESPACES
    #undef __errno_h  // later inclusion of errno.h must do using std::*
  #endif

#endif
