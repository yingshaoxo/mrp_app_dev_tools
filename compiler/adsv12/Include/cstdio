#ifndef __cstdio
#define __cstdio

  #define __STDIO_NO_EXPORTS 1
  #include <stdio.h>
  #undef __STDIO_NO_EXPORTS

  #ifdef __EDG_RUNTIME_USES_NAMESPACES
    #undef __stdio_h  // later inclusion of stdio.h must do using std::*
  #endif

#endif
