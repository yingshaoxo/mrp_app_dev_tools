#define __cstdlib

  #define __STDLIB_NO_EXPORTS 1
  #include <stdlib.h>
  #undef __STDLIB_NO_EXPORTS

  #ifdef __EDG_RUNTIME_USES_NAMESPACES
    #undef __stdlib_h  // later inclusion of stdlib.h must do using std::*
  #endif
