#ifndef __cstdint
#define __cstdint

  #define __STDINT_NO_EXPORTS 1
  #include <stdint.h>
  #undef __STDINT_NO_EXPORTS

  #ifdef __EDG_RUNTIME_USES_NAMESPACES
    #undef __stdint_h  // later inclusion of stdint.h must do using std::*
  #endif

#endif
